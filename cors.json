{
  "cors": [
    {
      "origin": ["http://localhost:8080", "http://localhost:3000", "http://127.0.0.1:8080"],
      "method": ["GET", "POST", "PUT", "DELETE", "HEAD", "OPTIONS"],
      "maxAgeSeconds": 3600,
      "responseHeader": [
        "Content-Type",
        "Authorization",
        "Content-Length",
        "User-Agent",
        "x-goog-resumable",
        "x-goog-meta-*"
      ]
    }
  ]
}