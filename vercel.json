    { "handle": "filesystem" },
    {
      "src": "/login",
      "dest": "/index.html"
    },
    {
      "src": "/register",
      "dest": "/index.html"
    },
    {
      "src": "/chat",
      "dest": "/index.html"
    },
    {
      "src": "/profile",
      "dest": "/index.html"
    },
    {
